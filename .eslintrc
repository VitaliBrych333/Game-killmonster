{
  "parser": "babel-eslint",
  "rules": {
      "no-console":0,
      "linebreak-style": 0,
      "indent": ["error", 4],
      "no-alert": 0,
      "prefer-destructuring": ["error", {"object": false, "array": false}],
      "no-param-reassign": 0,
      "allowNamedFunctions": true,
      "camelcase": [0, {"properties": "never"}],
      "no-continue": 0,
      "no-unused-vars": 0,
      "no-use-before-define": 0,
      "no-plusplus": 0,
      "no-mixed-operators": 0,
      "guard-for-in": 0,
      "no-restricted-syntax": ["error", "WithStatement", "BinaryExpression[operator='in']"],
      "no-multi-assign": 0,
      "no-underscore-dangle": 0,
      "object-shorthand": 0,
      "prefer-template": 0,
      "no-useless-concat": 0,
      "arrow-parens": 0,
      "no-loop-func": 0,
      "no-restricted-globals": 0,
      "no-path-concat": 0

  },
  "extends": ["airbnb-base"],
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "document": false
  }  
}
